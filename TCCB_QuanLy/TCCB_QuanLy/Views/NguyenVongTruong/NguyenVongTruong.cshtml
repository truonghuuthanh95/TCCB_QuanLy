
@{
    ViewBag.Title = "Nguyện Vọng Trường";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <ol class="breadcrumb">
        <li>
            <a href="#">
                <em class="fa fa-home"></em>
            </a>
        </li>
        <li class="active">Nguyện Vọng Trường</li>
    </ol>
</div><!--/.row-->
<div class="row">
